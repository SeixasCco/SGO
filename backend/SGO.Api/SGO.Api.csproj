    <PackageReference Include="Swashbuckle.AspNetCore" Version="9.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SGO.Infrastructure\SGO.Infrastructure.csproj" />
  </ItemGroup>

